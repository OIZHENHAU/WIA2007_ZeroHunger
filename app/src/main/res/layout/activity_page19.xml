<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/page19_1"
    tools:context=".Part2.page19">

    <Button
        android:id="@+id/page19_registerBtnP2"
        android:layout_width="130dp"
        android:layout_height="50dp"
        android:backgroundTint="#576AD7"
        android:text="Register"
        android:textSize="18sp"
        app:cornerRadius="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.487"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.842" />

    <LinearLayout
        android:layout_width="415dp"
        android:layout_height="50dp"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.567"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <Button
            android:id="@+id/page19_aboutUsBtnP2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:backgroundTint="#AEEAF0"
            android:text="About us"
            android:textColor="#000000"
            app:cornerRadius="0dp" />

        <Button
            android:id="@+id/page19_resourcesBtnP2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:backgroundTint="#AEEAF0"
            android:text="Resources"
            android:textColor="#000000"
            app:cornerRadius="0dp" />

        <Button
            android:id="@+id/page19_donationBtnP2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:backgroundTint="#30D1E6"
            android:text="Donation"
            android:textColor="#000000"
            app:cornerRadius="0dp" />

        <Button
            android:id="@+id/page19_getHelpBtnP2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:backgroundTint="#AEEAF0"
            android:text="Help"
            android:textColor="#000000"
            app:cornerRadius="0dp" />
    </LinearLayout>

    <Button
        android:id="@+id/page19_readMoreBtnP2"
        android:layout_width="130dp"
        android:layout_height="48dp"
        android:backgroundTint="#576AD7"
        android:text="Read more"
        android:textSize="16sp"
        app:cornerRadius="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.487"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

</androidx.constraintlayout.widget.ConstraintLayout>